"VertexLitGeneric"
{
	"$basetexture" "models/chuckgreene/shirt"
	// $bumpmap is a shared 16x16 flat normal map, and is needed or lightwarp won't shade correctly.
	"$bumpmap"     "models/chuckgreene/shirt_n"
	//"$detailscale" "1"
	//"$detailblendfactor" .01
	//"$detailblendmode" 6

	"$yellow" "0"
	"$one" "1"


	"$phong" "1"
	"$phongexponent" "10.1"
	"$phongboost" ".1"	
	//"$lightwarptexture" "models/player/pyro/pyro_lightwarp"
	"$phongfresnelranges"	"[.3 .1 .8]"
	"$halflambert" "1"

	// Rim lighting parameters
	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "12.4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "1.1"										// Boost for ambient cube component of rim lighting
}

