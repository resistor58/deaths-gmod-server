// envmaptint_fix
"vertexlitgeneric"
{
	"$basetexture" "stone/stonewall051b"
	"$surfaceprop" "concrete"
	"%keywords" "c17downtown"
	"$bumpmap" "stone/stonewall051a_normal"
	"$normalmapalphaenvmapmask" 1
	"$envmapcontrast" 1
	"$envmapsaturation" 1
	"$envmaptint" "[ .56 .56 .56 ]"
	"$envmap" "env_cubemap"


	"vertexlitgeneric_dx9"
	{
		"$normalmapalphaenvmapmask" 1
		"$envmaptint" "[ .1 .1 .1]"
	}

	"vertexlitgeneric_dx8"
	{
		"$nodiffusebumplighting" 1
		"$normalmapalphaenvmapmask" 1
	}
	
	"vertexlitgeneric_hdr_dx9"
	{
		// put hdr override stuff here
		"$envmaptint" "[ .1 .1 .1]"
	}
	
	
}
